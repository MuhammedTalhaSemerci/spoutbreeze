{
  "name": "spoutbreeze/spoutbreeze-web",
  "description": "API & Web-Application Management Console",
  "type": "project",
  "autoload": {
    "psr-4": {
      "\\": "app/src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "app/src",
      "tests/src/"
    ]
  },
  "require": {
    "ext-json": "*",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-bcmath": "*",
    "bcosca/fatfree-core": "3.7.3",
    "ikkez/f3-cortex": "^1.7",
    "ikkez/f3-mailer": "^1.2",
    "respect/validation": "^1.1",
    "nette/utils": "^3.1.3",
    "tracy/tracy": "^2.7",
    "matthiasmullie/minify": "^1.3",
    "monolog/monolog": "^2.1",
    "robmorgan/phinx": "^0.12",
    "symfony/yaml": "^5.1",
    "league/uri": "^6.3"
  },
  "require-dev": {
    "ext-xdebug": ">=2.3.3",
    "kint-php/kint": "^3.3",
    "fakerphp/faker": "^1.13",
    "friendsofphp/php-cs-fixer": "^2.16.7",
    "phpunit/php-code-coverage": "^7.0.10",
    "kevinlebrun/colors.php": "^1.0.3",
    "gabrielelana/byte-units": "^0.5",
    "phpmetrics/phpmetrics": "^2.7.4"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": false,
    "process-timeout": 600,
    "preferred-install": "dist",
    "github-protocols": [
      "https"
    ]
  },
  "suggest": {
    "xfra35/f3-cron": "Job scheduling for the PHP Fat-Free Framework",
    "ikkez/f3-mailer": "Email class with error and bounce handling"
  },
  "license": "PRIVATE",
  "authors": [
    {
      "name": "RIADVICE",
      "email": "devops@riadvice.tn"
    }
  ]
}
